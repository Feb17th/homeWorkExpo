{
  "expo": {
    "name": "QR Place",
    "slug": "expo-redux-template",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hpcc.expo-redux-template",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow QR Place to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hpcc.expo_redux_template",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photo to let you scan the QR contained in it."
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow QR Place to access camera."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow QR Place to access your camera."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "7f48ce6c-b5c4-4897-ac68-871bd43750ec"
      }
    }
  }
}
